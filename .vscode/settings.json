// Place your settings in this file to overwrite the default settings
{
	"search.excludeFolders": [
        ".git",
        "Library",
        "obj",
        "Temp",
        "ProjectSettings"
    ],
    "workbench.editor.enablePreview": false,
    "editor.wordWrap": "on",
    "vim.disableAnnoyingNeovimMessage": true,
    "workbench.colorTheme": "Solarized Dark",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.idea":true,
        "**/.DS_Store": true,
        "**/*.meta":true,
        "**/*.anim":true,
        "**/*.mat":true,
        "**/*.controller":true,
        "**/*.prefab":true,
        "**/*.asset":true
    },
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.showUpdateMessage": false,
    "go.gopath": "$GOPATH",
    "go.goroot": "/usr/local/opt/go/libexec",
    "csharp.format.enable": false,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "materialTheme.cache.workbench.settings": {},
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#00BCD4",
        "list.activeSelectionForeground": "#00BCD4",
        "list.inactiveSelectionForeground": "#00BCD4",
        "list.highlightForeground": "#00BCD4",
        "scrollbarSlider.activeBackground": "#00BCD450",
        "editorSuggestWidget.highlightForeground": "#00BCD4",
        "textLink.foreground": "#00BCD4",
        "progressBar.background": "#00BCD4",
        "pickerGroup.foreground": "#00BCD4",
        "tab.activeBorder": "#00BCD4",
        "notificationLink.foreground": "#00BCD4",
        "statusBar.background": "#abce00",
        "statusBar.noFolderBackground": "#abce00",
        "statusBar.debuggingBackground": "#abce00"
    },
    "markdown-run-snippet.mdTypeToTemplateMap": {
        "rust": "fn main() {\n    $snippet\n}",
        "c++": "#include <iostream>\n#include <string>\nint main(void) {\n    $snippet\n    return 0;\n}"
    },
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.easymotion": true,
    "vim.visualstar": true,
    "editor.formatOnType": true,
    "editor.renderWhitespace": "boundary",
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[ruby]": {
        "editor.tabSize":2
    },
    "[python]": {
        "editor.tabSize":4
    },
    "[makefile]": {
        "editor.insertSpaces": false
    },
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true
    },
    "[css]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true
    },
    "[markdown]": {
        "editor.formatOnSave": true,
        "editor.wordWrap": "on",
        "editor.renderWhitespace": "all",
        "editor.acceptSuggestionOnEnter": "off"
    },
    "editor.rulers": [80],
    "files.insertFinalNewline": true,
    "emmet.triggerExpansionOnTab": true,
    "files.associations": {
        ".zshrc.*": "shellscript"
    },
    "extensions.ignoreRecommendations": true,
}
